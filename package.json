{
  "name": "leads-manager",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint",
    "typecheck": "tsc --noEmit",
    "check:styles": "node scripts/check-inline-styles.mjs",
    "check:all": "npm run typecheck && npm run lint && npm run check:styles",
    "seed:prod": "node scripts/seed_prod.mjs",
    "deploy": "npm run check:all && git add -A && git commit -m \"deploy\" && git push origin master && npx vercel --prod",
    "db:health": "node -e \"const m=require('mongoose');require('dotenv').config({path:'.env.local'});m.connect(process.env.MONGODB_URI).then(()=>{console.log('✅ MongoDB connected');process.exit(0)}).catch(e=>{console.error('❌ MongoDB failed:',e.message);process.exit(1)})\""
  },
